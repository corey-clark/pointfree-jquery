{
  "name": "pointfree-jquery",
  "version": "1.8.11",
  "description": "Curried a few common jQuery methods to allow them to be written in a point free fashion.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha src/index.test.js -w --compilers js:babel-register",
    "build": "babel --out-dir dist --ignore *.test.js src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corey-clark/pointfree-jquery.git"
  },
  "keywords": [
    "pointfree",
    "jquery"
  ],
  "author": "Corey Clark <info@coreyclarkjr.com> (http://coreyclarkjr.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corey-clark/pointfree-jquery/issues"
  },
  "homepage": "https://github.com/corey-clark/pointfree-jquery#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-plugin-transform-strict-mode": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.8.0",
    "chai": "3.5.0",
    "mocha": "2.4.5"
  },
  "babel": {
    "presets": ["es2015"],

    "plugins": [
      [
        "transform-strict-mode",
        {
          "strict": false
        }
      ]
    ]
  }
}
